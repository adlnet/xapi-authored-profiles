    "type": "Profile",
    "prefLabel": {
        "en": "ADL Vocabulary"
    },
    "definition": {
      "en": "The ADL vocabulary list includes Verbs and Activity Types used to represent common learning experiences in xAPI."
    },
    "conformsTo": "https://w3id.org/xapi/profiles#1.0.0",
    "versions": [
        {
            "id": "https://w3id.org/xapi/adl/v1.0",
